userNumber = input('Enter the marks: ');
disp(['You entered: ', num2str(userNumber)]);

if (userNumber > 100 || userNumber < 0)
    disp('Invalid mark')
    
elseif userNumber >= 90
    disp('A+')

elseif userNumber >= 80
    disp('A')

elseif userNumber >= 70
    disp('B')

elseif userNumber >= 60
    disp('C')

elseif userNumber >= 50
    disp('D')

else 
    disp('F')
end
